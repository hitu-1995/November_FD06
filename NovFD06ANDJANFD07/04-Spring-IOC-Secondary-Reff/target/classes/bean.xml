<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">
       <bean id="adr" class="com.cjc.in.Address">
            <property name="pinCode" value="42242"/>
            <property name="city" value="Pune"/>
            <property name="area" value="ShivajiNagar"/>
       </bean>

       <bean id="stu" class="com.cjc.in.Student">
          <property name="rollno" value="101"/>
          <property name="name" value="Swapnil Joshi"/>
          <property name="address" ref="adr"/>
       </bean>

</beans>
